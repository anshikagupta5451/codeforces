#include<bits/stdc++.h>
using namespace std;
int main()
{
    
    int m,n,a;
    cin>>m>>n>>a;
   // cout<<m<<n<<a;
    int x,y;
    int r=m%a;
    int s=n%a;
    int sum=0;
    if(r!=0 && s!=0)
    {
        x=m/a+1;
        y=n/a+1;
    }
    else if(r==0 && s==0)
    {
        x=m/a;
        y=n/a;
    }
    else if(r==0 && s!=0)
    {
        x=m/a;
        y=n/a+1;
    }
    else
    {
        x=m/a+1;
        y=n/a;
    }
    sum=sum+1;
    
    cout<<x*y;
    
   // cout<<r;
    
}
